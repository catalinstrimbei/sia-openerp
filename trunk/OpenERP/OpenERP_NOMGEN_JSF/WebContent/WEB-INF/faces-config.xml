<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<application>
		<message-bundle>resources.application</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
	</application>
	<managed-bean>
		<managed-bean-name>FormProdus</managed-bean-name>
		<managed-bean-class>org.open.erp.ui.FormPersoana</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>FormDocument</managed-bean-name>
		<managed-bean-class>org.open.erp.ui.FormDocument</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>FormMaterial</managed-bean-name>
		<managed-bean-class>org.open.erp.ui.FormMaterial</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>FormPersoana</managed-bean-name>
		<managed-bean-class>org.open.erp.ui.FormPersoana</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<lifecycle>
		<phase-listener>org.open.erp.ui.PhaseTracker</phase-listener>
	</lifecycle>	
</faces-config>
