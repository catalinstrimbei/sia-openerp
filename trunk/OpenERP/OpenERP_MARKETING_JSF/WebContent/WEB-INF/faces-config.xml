<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <converter>
  <converter-id>CustomDateConverter</converter-id>
  <converter-class>org.open.erp.ui.CustomDateConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>Promotii</managed-bean-name>
  <managed-bean-class>org.open.erp.ui.ListaPromotiiImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Campanii</managed-bean-name>
  <managed-bean-class>org.open.erp.ui.ListaCampaniiImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Discounturi</managed-bean-name>
  <managed-bean-class>org.open.erp.ui.ListaDiscounturiImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ChestionarIntrebari</managed-bean-name>
  <managed-bean-class>org.open.erp.ui.ListaChestionareIntrebariImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <application>
  <message-bundle>resources.application</message-bundle>
  <locale-config>
   <default-locale>en</default-locale>
  </locale-config>
 </application>
 <lifecycle>
  <phase-listener>org.open.erp.ui.PhaseTracker</phase-listener>
 </lifecycle>
</faces-config>
